#include<stdio.h>
int main()
{
	int n,i;
	printf("Enter the no of elements : ");
	scanf("%d",&n);
	
	int numbers[n];
	printf("Enter the elements : ");
	for(i=0;i<n;i++){
		scanf("%d",&numbers[i]);
	}
	
	for(i=0;i<n;i++)
	{
		if(numbers[i]%2==0)
		{
			printf("%d is even\n",numbers[i]);
		}
		else
		{
			printf("%d is odd\n",numbers[i]);
		}
	}
	
}
